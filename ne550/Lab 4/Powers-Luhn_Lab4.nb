       RowBox[{"Log", "[", 
        RowBox[{"1", "-", 
         RowBox[{"ap", "[", 
          RowBox[{"[", "i", "]"}], "]"}]}], "]"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", 
        RowBox[{"Length", "[", "ap", "]"}]}], "}"}]}], "]"}], "//", "N"}]}], 
