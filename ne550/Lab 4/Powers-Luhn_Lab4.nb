  RowBox[{"(*", " ", 
   RowBox[{"mg", "/", 
    SuperscriptBox["cm", "3"]}], " ", "*)"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ap", "=", 
   RowBox[{"Table", "[", 
    RowBox[{"i", ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "0.1", ",", "0.8", ",", "0.1"}], "}"}]}], "]"}]}], 
  " ", 
  RowBox[{"(*", " ", 
   RowBox[{"Attenuation", " ", "Percentages"}], " ", "*)"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"at", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"-", "1"}], 
       RowBox[{"mu", "*", "density"}]], "*", 
      RowBox[{"Log", "[", 
       RowBox[{"1", "-", 
        RowBox[{"ap", "[", 
         RowBox[{"[", "i", "]"}], "]"}]}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", 
       RowBox[{"Length", "[", "ap", "]"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Multicolumn", "[", 
  RowBox[{
   RowBox[{"Join", "[", 
    RowBox[{"ap", ",", "at"}], "]"}], ",", "2"}], "]"}]}], "Input",
