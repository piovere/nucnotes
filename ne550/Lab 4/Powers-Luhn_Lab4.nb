  RowBox[{"e1", "=", "100"}], " ", 
  RowBox[{"(*", " ", "keV", " ", "*)"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"mu0", "=", 
   RowBox[{"0.2018", " ", "*", " ", 
    SuperscriptBox["10", 
     RowBox[{"-", "3"}]]}]}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{
    SuperscriptBox["cm", "2"], "/", "mg"}], " ", "*)"}], 
  ";"}], "\[IndentingNewLine]", 
