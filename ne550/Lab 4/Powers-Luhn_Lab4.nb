     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"90", "Y"}], "&"}], "unc"}], "=", "nds"}]}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Y90BetaEnergy", "=", 
     RowBox[{"{", 
      RowBox[{"25.07", ",", "185.61", ",", "933.7"}], "}"}]}], " ", 
