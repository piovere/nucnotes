Cell["\<\
5. Using the value found in question 4, calculate the thickness of Aluminum \
required to absorb between 10-80% of the 88keV gamma-rays in steps of 10%.\
\>", "Text",
 CellChangeTimes->{{3.695572298800321*^9, 3.69557235570415*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"density", "=", "2700"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{"mg", "/", 
    SuperscriptBox["cm", "3"]}], " ", "*)"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ap", "=", 
   RowBox[{"Table", "[", 
    RowBox[{"i", ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "0.1", ",", "0.8", ",", "0.1"}], "}"}]}], "]"}]}], 
  " ", 
  RowBox[{"(*", " ", 
   RowBox[{"Attenuation", " ", "Percentages"}], " ", "*)"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"at", "=", 
   RowBox[{
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"-", "1"}], 
        RowBox[{"mu", "*", "density"}]], "*", 
       RowBox[{"Log", "[", 
        RowBox[{"1", "-", 
         RowBox[{"ap", "[", 
          RowBox[{"[", "i", "]"}], "]"}]}], "]"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", 
        RowBox[{"Length", "[", "ap", "]"}]}], "}"}]}], "]"}], "//", "N"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Multicolumn", "[", 
  RowBox[{
   RowBox[{"Join", "[", 
