     RowBox[{"half", " ", "life", " ", "of", " ", "Sr"}], "-", 
     RowBox[{"90", " ", "28.79", "y"}]}], " ", "*)"}], "\[IndentingNewLine]", 
   
   RowBox[{
    RowBox[{"Sr90HalfLife", "=", 
     RowBox[{"28.79", "*", "365.241", "*", "24"}]}], ";", " ", 
    RowBox[{"(*", 
