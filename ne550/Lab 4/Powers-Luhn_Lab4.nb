 StyleBox[
  RowBox[{"Gamma", "-", 
   RowBox[{"ray", " ", "attenuation"}]}], "Subchapter"]], "Input",
 CellChangeTimes->{{3.695731466982058*^9, 3.695731471208642*^9}}],

